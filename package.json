{
  "name": "pp",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.6",
    "@mdi/font": "7.4.47",
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-axios": "^3.5.2",
    "vue3-toastify": "^0.2.8",
    "vuetify": "^3.9.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-vuetify": "^4.0.0",
    "husky": "^9.0.0",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.0.0",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.8.0",
    "unplugin-vue-router": "^0.14.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-plugin-vuetify": "^2.1.1",
    "vitest": "^2.0.0",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,scss,css}": [
      "prettier --write"
    ]
  }
}
